<div class="reference-inner-container">
  <div class="reference <%= reference.data.type %>">
    <a href="<%= config.http_prefix + reference.data.type + '.html#' + reference.data.title %>">
      <h4 class="reference-header"><%= reference.data.title %></h4>
      <p class="reference-description"><%= reference.data.description %></p>
    </a>
  </div>
</div> 